  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "target": "es2022",
    "lib": ["es2022"],
    "types": [
      "node"
    ]
  },
  "include": [
    "vite.config.ts"
  ]
}